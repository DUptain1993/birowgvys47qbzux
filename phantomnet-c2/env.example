# PhantomNet C2 Server Environment Configuration
# Copy this file to .env and update the values

# Flask Configuration
FLASK_ENV=development
SECRET_KEY=your-secret-key-here
DEBUG=False

# Database Configuration
DATABASE_URL=sqlite:///phantom_c2.db
# For PostgreSQL: postgresql://user:password@localhost/phantomnet
# For MySQL: mysql://user:password@localhost/phantomnet

# Server Configuration
HOST=0.0.0.0
PORT=8443
SSL_CERT_PATH=/path/to/cert.pem
SSL_KEY_PATH=/path/to/key.pem

# API Keys for Threat Intelligence
SHODAN_API_KEY=your-shodan-api-key
CENSYS_API_ID=your-censys-api-id
CENSYS_API_SECRET=your-censys-api-secret
VIRUSTOTAL_API_KEY=your-virustotal-api-key
ABUSEIPDB_API_KEY=your-abuseipdb-api-key
OTX_API_KEY=your-otx-api-key

# C2 Server Configuration
C2_SERVER_IP=your-server-ip
C2_SERVER_PORT=8443

# DuckDNS Configuration
DUCKDNS_DOMAIN=your-domain
DUCKDNS_TOKEN=your-duckdns-token

# Security Settings
MAX_LOGIN_ATTEMPTS=5
LOCKOUT_DURATION=900
PASSWORD_MIN_LENGTH=8
SESSION_TIMEOUT=86400

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=phantom.log

# Background Task Configuration
BACKGROUND_TASK_INTERVAL=300
BOT_INACTIVE_TIMEOUT=7200
DATA_RETENTION_DAYS=30

# Payload Generation
MSFVENOM_PATH=/usr/bin/msfvenom

# Rate Limiting
RATE_LIMIT_ENABLED=True
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600
